{
  "scripts": {
    "dev": "concurrently \"npm run watch-css\" \"npm run start-server\" \"npm run watch-webpack\"\n",
    "start-server": "nodemon server.js\n",
    "build:css": "npx @tailwindcss/cli -i ./src/input.css -o ./dist/output.css\n",
    "watch-css": "npx @tailwindcss/cli -i ./src/input.css -o ./dist/output.css --watch\n",
    "watch-webpack": "webpack --watch"
  },
  "dependencies": {
    "@fontsource-variable/source-code-pro": "^5.2.5",
    "@tailwindcss/cli": "^4.0.15",
    "d3": "^7.9.0",
    "nodemon": "^3.1.9",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "express": "^5.1.0",
    "webpack-cli": "^6.0.1"
  }
}
